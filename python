import turtle
t = turtle.Pen()
t.speed(3)

t.begin_fill()
t.color(0,0,0.8)
t.forward(30)
t.right(90)
t.forward(10)
t.right(90)
t.forward(20)
t.left(90)
t.forward(35)
t.left(90)
t.forward(20)
t.right(90)
